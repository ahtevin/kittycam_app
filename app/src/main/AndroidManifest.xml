<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kittycam_final">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppThem">
        <activity
            android:name=".livecast"
            android:label="@string/title_activity_livecast"
            android:theme="@style/AppThem.NoActionBar"></activity>
        <activity
            android:name=".logs"
            android:label="@string/title_activity_logs"
            android:theme="@style/AppThem.NoActionBar" />
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppThem.NoActionBar" />
        <activity
            android:name=".page_home"
            android:label="@string/title_activity_page_home"
            android:theme="@style/AppThem.NoActionBar" />
        <activity android:name=".menupage" />
        <activity android:name=".login" />
        <activity android:name=".sign_up" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>